<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.po">
	<class name="Role" table="t_role" lazy="false">
		<id name="rid" column="rid">
			<generator class="native"></generator>            
        </id>	
       <property name="rname"></property>
       
       <property name="admin"></property>
       <property name="receive"></property>
       <property name="ticket"></property>
       <property name="delivery"></property>
       <property name="finance"></property>    
       <property name="business"></property>  
       <property name="data"></property>
       <property name="manager"></property>     
	</class>
</hibernate-mapping>




